100 'SAVE"VP_VAL.ASC",A
110 SCREEN 0:WIDTH 40:DEFINT A-Z
120 J=0:L=0:PS=0:PV=0:PW=0:V$=""
130 V$="I%":L=2:I%=&HABCD
140 PV=VARPTR(I%):GOSUB 220
150 V$="S!":L=4:S!=987654!
160 PV=VARPTR(S!):GOSUB 220
170 V$="D#":L=8:D#=98765432109876#
180 PV=VARPTR(D#):GOSUB 220
190 V$="S$":L=3:S$="abcdefg"
200 PV=VARPTR(S$):GOSUB 220:GOSUB 290
210 END
220 '
230 PRINT:PRINT V$;":";HEX$(PV)
240 FOR J=0 TO L-1
250  PRINT RIGHT$(HEX$(PEEK(PV+J)+256),2);" ";
260 NEXT
270 PRINT
280 RETURN
290 '
300 L=PEEK(PV):PW=VARPTR(PS)
310 POKE PW  ,PEEK(PV+1)
320 POKE PW+1,PEEK(PV+2)
330 PRINT CHR$(34);
340 FOR J=0 TO L-1
350  PRINT CHR$(PEEK(PS+J));
360 NEXT
370 PRINT CHR$(34)
380 RETURN
