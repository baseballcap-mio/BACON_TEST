100 'SAVE"PUT00000.ASC",A
110 DEFINT A-Z
120 OPEN"PUT00000.DAT" AS #1 LEN=2
130 FIELD #1,2 AS R$
140 FOR J=1 TO 7
150  A=0:M=1:READ D$
160  FOR I=1 TO 14
170   IF MID$(D$,I,1)="O" THEN A=A OR M
180   M=M*2
190  NEXT I
200  IF MID$(D$,15,1)="O" THEN A=A OR M
210  M=&H8000
220  IF MID$(D$,16,1)="O" THEN A=A OR M
230  LSET R$=MKI$(A):PUT #1
240 NEXT J
250 CLOSE
260 END
270 '
280 DATA "O   O  OO  O   O"
290 DATA "OO OO O  O O   O"
300 DATA "O O O O     O O "
310 DATA "O   O  OO    O  "
320 DATA "O   O    O  O O "
330 DATA "O   O O  O O   O"
340 DATA "O   O  OO  O   O"
